    "@types/react-dom": "18.2.25",
    "@types/three": "^0.182.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "three": "^0.182.0",
    "tsup": "^8.5.1",
    "typescript": "5.9.3"
