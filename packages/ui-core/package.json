{
  "name": "@mhersztowski/scene3d-ui-core",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "react": "18.2.0",
    "@types/react": "18.2.79",
    "tsup": "^8.5.1",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}
